(defn fizzbuzz [n]
    (cond 
        (divides? 3 n ) "fizz"
        (divides? 5 n ) "buzz"
        (divides? 15 n) "gotcha!"
        :else ""
       )) 
(defn divides? [divisor n]
   (=(mod n divisor)0))
(defn fizzbuzz [n]
    (cond 
        (divides? 3 n ) "fizz"
        (divides? 5 n ) "buzz"
        (divides? 15 n) "gotcha!"
        :else ""
       )) 
(fizzbuzz 2)
(fizzbuzz 45)
(defn fizzbuzz [n]
    (cond 
        (divides? 15 n ) "fizz"
        (divides? 5 n ) "buzz"
        (divides? 3 n) "gotcha!"
        :else ""
       )) 
(fizzbuzz 2)
(fizzbuzz 45)
(defn fizzbuzz [n]
    (cond 
        (divides? 15 n ) "gotcha!"
        (divides? 5 n ) "buzz"
        (divides? 3 n) "fizz"
        :else ""
       )) 
(fizzbuzz 2)
(fizzbuzz 45)
(fizzbuzz 48)
