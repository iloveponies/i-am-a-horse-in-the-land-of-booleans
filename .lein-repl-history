(defn leap-year? [year]
  (cond 
      (not (divides? year 4)) "niet deelbaar door 4 "
      (not (divides? year 100)) "niet deelbaar door 100"
      (not (divides? year 400)) "niet deelbaar door 400"
      :else "deelbaar" ))
(defn divides? [divisor n]
   (=(mod n divisor)0))
(defn leap-year? [year]
  (cond 
      (not (divides? year 4)) "niet deelbaar door 4 "
      (not (divides? year 100)) "niet deelbaar door 100"
      (not (divides? year 400)) "niet deelbaar door 400"
      :else "deelbaar" ))
(leap-year? 400)
(divides 400 4) 
(divides? 400 4) 
( mod 400 4) 
(= mod 400 4) 0)
( = (mod 400 4)0)
(divides? 400 4) 
(defn leap-year? [year]
  (cond 
      (not (divides? 4 year)) "niet deelbaar door 4 "
      (not (divides? 100 year )) "niet deelbaar door 100"
      (not (divides? 400 year)) "niet deelbaar door 400"
      :else "deelbaar" ))
(leap-year? 400)
quit
